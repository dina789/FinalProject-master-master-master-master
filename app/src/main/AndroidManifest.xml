<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zizo.myapplication">
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignIn_Activity" />
        <activity android:name=".SignUp_Activity" />
        <activity android:name=".SignIn_Activity$PlaceModel_Activity" />
        <activity android:name=".Adapters.Place_adapter" />
        <activity android:name=".DataModel.placemodel_Activity" />
        <activity android:name=".UserDetails" />
        <activity android:name=".details" />
        <activity android:name=".Favorite_Activity"></activity>
    </application>

</manifest>